{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/_image/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/about/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/admin/articles/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/admin/login/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/admin/manage/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/admin/settings/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/admin/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/api/blog-files/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/api/create-blog/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/og\\.png/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/posts(?:/(.*?))?/index\\.png/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/.*$",
			"dest": "/404.html",
			"status": 404
		}
	]
}